{
  "name": "@concise-online/co-ui",
  "version": "0.0.2",
  "description": "A React.js UI Toolkit for Web",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "typings": "cjs/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concise-online/co-ui.git"
  },
  "keywords": [
    "react",
    "component",
    "ui",
    "front-end",
    "framework",
    "components"
  ],
  "authors": [
    "liujinyang <liujinyang@bigtiger.me> (https://github.com/jinyang1994)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concise-online/co-ui/issues"
  },
  "homepage": "https://github.com/concise-online/co-ui",
  "scripts": {
    "start": "NODE_ENV=development bisheng start -c ./site/bisheng.config.js",
    "site": "rm -rf ./_site && mkdir ./_site && cp -i CNAME ./_site  && bisheng build -c ./site/bisheng.config.js",
    "gh-pages": "bisheng gh-pages -p -r upstream",
    "compile": "gulp compile",
    "compile:esm": "gulp compile:esm",
    "compile:cjs": "gulp compile:cjs",
    "dist": "gulp dist",
    "clean": "gulp clean",
    "lint": "npm run lint:ts && npm run lint:es && npm run lint:demo && npm run lint:style",
    "lint:ts": "tslint -c tslint.json -p tsconfig.json 'components/**/*.{ts,tsx}'",
    "lint:es": "eslint site build --ext '.js,.jsx'",
    "lint:demo": "cross-env RUN_ENV=DEMO eslint components --ext '.md'",
    "lint:style": "stylelint '{site,components}/**/*.less' --syntax less",
    "test": "jest -c .jest.json --no-cache",
    "test:cov": "jest -c .jest.json --coverage --no-cache",
    "ci": "npm run lint && jest -c .jest.json --ci --coverage"
  },
  "pre-commit": [
    "lint"
  ],
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@mdi/js": "^3.8.95",
    "@mdi/react": "^1.2.1",
    "classnames": "^2.2.6",
    "react-intl": "^2.9.0",
    "warning": "^4.0.3"
  },
