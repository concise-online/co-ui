    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/warning": "^3.0.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
